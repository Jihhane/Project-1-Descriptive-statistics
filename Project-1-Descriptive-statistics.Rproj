library(readr)
ps_p1 <- read_csv("ps-p1.csv", col_types = cols(UnitsSold = col_number(), 
                                                UnitPrice = col_number(), UnitCost = col_number(), 
                                                TotalRevenue = col_number(), TotalCost = col_number(), 
                                                TotalProfit = col_number()))
View(ps_p1)
library(dplyr)
 Q4<- filter(ps_p1, Region == "Europe" & ItemType == "Beverages" )
 View(Q4)
 Q5 <- Q4[order(Q4$UnitsSold),]
 View(Q5)
 vegetables <- filter(ps_p1 , ItemType == "Vegetables")
View(vegetables) 
Q6<- median(vegetables $UnitsSold)
View(Q6)
Q7 <- mean(vegetables $UnitPrice)
View(Q7)
Q8 <- var(vegetables $UnitPrice)
View(Q8)
Q9<- sd(vegetables $UnitPrice)
View(Q9)
library(e1071)
Q10 <- moment(vegetables $UnitPrice, order=3, center=TRUE) 
View(Q10)
Q11 <- skewness(vegetables $UnitPrice)
View(Q11)
Q12 <- moment(vegetables $UnitPrice, order=4, center=TRUE)
View(Q12)
Q13 <- kurtosis(vegetables $ UnitPrice)  
View(Q13)
